<div style = "overflow: auto;" >
<h1>Welcome <%=current_user.first_name.capitalize%> <%=current_user.last_name.capitalize%> </h1>

	<table>
		<td style= "width: 60%">
			<img src=<%= @user_profile.profile_image_url%>  style="width:150px;height:150px;">
		</td>

		<td>
			<p>
				Phone No.:- <%=@user_profile.phone_no%><br/>
				Sex:- <%=@user_profile.sex%><br/>
				Date of Birth:- <%=@user_profile.date_of_birth%><br/>
				Address:- <%=@user_profile.address%><br/>
			</p>
			<p><%= link_to 'Edit', edit_user_profile_path(@user_profile) %></p>
		</td>
		
	</table>
</div>